# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Environment
ENVIRONMENT=dev

# LocalStack (for local development)
LOCALSTACK_ENDPOINT=http://localhost:4566

# Provider Configuration (stored in AWS Secrets Manager in production)
# Local development only - use fake values for LocalStack
YOUR_API_KEY=fake-api-key-for-localstack
WEBHOOK_HMAC_SECRET=your-webhook-secret-key

# CloudFront
CLOUDFRONT_DOMAIN=cdn.your-domain.com

# S3
S3_BUCKET_NAME=your-service-dev

# DynamoDB
DYNAMODB_TABLE_NAME=your-service-dev

# SQS
SQS_QUEUE_NAME=your-service-queue-dev
SQS_DLQ_NAME=your-service-dlq-dev